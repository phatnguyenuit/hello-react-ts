{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": true,
    "semicolon": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  }
}
